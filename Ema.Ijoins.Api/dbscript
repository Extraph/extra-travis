    (session_id ASC NULLS LAST)
    TABLESPACE pg_default;




-- Table: public.TBT_IJOIN_SCAN_QR_HIS

-- DROP TABLE public."TBT_IJOIN_SCAN_QR_HIS";

CREATE TABLE public."TBT_IJOIN_SCAN_QR_HIS"
(
    id bigint NOT NULL,
    file_id integer NOT NULL,
    course_type_id integer NOT NULL,
    course_id integer NOT NULL,
    session_id integer NOT NULL,
    start_date_time timestamp without time zone NOT NULL,
    end_date_time timestamp without time zone NOT NULL,
    user_id text COLLATE pg_catalog."default" NOT NULL,
    registration_status text COLLATE pg_catalog."default" NOT NULL,
    check_in_date_time timestamp without time zone,
    check_out_date_time timestamp without time zone,
    createdatetime timestamp without time zone NOT NULL,
    updatedatetime timestamp without time zone NOT NULL,
    update_by text COLLATE pg_catalog."default",
    createhisdatetime timestamp without time zone NOT NULL DEFAULT now(),
    CONSTRAINT "TBT_IJOIN_SCAN_QR_HIS_pkey" PRIMARY KEY (course_id, session_id, start_date_time, end_date_time, user_id),
    CONSTRAINT "fk_TBM_COURSE_TYPE_id_his" FOREIGN KEY (course_type_id)
        REFERENCES public."TBM_COURSE_TYPE" (id) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION,
    CONSTRAINT "fk_TBM_COURSE_course_id_his" FOREIGN KEY (course_id)
        REFERENCES public."TBM_COURSE" (course_id) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION,
    CONSTRAINT "fk_TBM_SESSION_session_id_his" FOREIGN KEY (session_id)
        REFERENCES public."TBM_SESSION" (session_id) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION
)

TABLESPACE pg_default;

ALTER TABLE public."TBT_IJOIN_SCAN_QR_HIS"
    OWNER to ema_user;
